
<!DOCTYPE html>
<html>
<head>
    <title>HPRS :: @ViewBag.Title</title>
    <link rel="stylesheet" href="~/Areas/HPRS/assets/lib/bootstrap/dist/css/bootstrap.min.css">
    <link href="~/Areas/HPRS/assets/css/keen-dashboards.css" rel="stylesheet" />
</head>
<body class="application">

    <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="/hprs">
                    <span class="glyphicon glyphicon-chevron-left"></span>
                </a>
                <a class="navbar-brand" href="/hprs">Harris Performance Reporting System</a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav navbar-left">
                    <li><a href="https://keen.io">Home</a></li>
                </ul>
            </div>
        </div>
    </div>

    <div class="container-fluid">
        @RenderBody()
    </div>

    <script src="~/Areas/HPRS/assets/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/Areas/HPRS/assets/lib/bootstrap/dist/js/bootstrap.min.js"></script>

    <script src="~/Areas/HPRS/assets/lib/holderjs/holder.js"></script>
    <script>
        Holder.add_theme("white", { background: "#fff", foreground: "#a7a7a7", size: 10 });
    </script>

    <s@*cript src="~/Areas/HPRS/assets/lib/keen-js/dist/keen.min.js"></script>*@
    <script src="~/Areas/HPRS/assets/js/meta.js"></script>
    <script src="~/Areas/HPRS/assets/js/keen.dashboard.js"></script>

</body>
</html>
